<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>

    <!--
        0 => No log,
        1 => Exception only,
        2 => Exception and Debug
        3 => Exception, Debug and Trace-->
        <add key="LogLevel" value="3"/>

    <!--
        0 => file, 
        1 => Sql db-->
    <add key="LogSource" value="1"/>
    <add key="LogName" value="ERent"/>
    <add key="LogCapacity" value="500"/>
    <add key="AdditinalHour" value="0"/>
    <add key="LogPath" value="D:\Obeidat Logger"/>
    <add key="LogConnection" value="Data Source=DESKTOP-ARHEKDS\SQLEXPRESS;Initial Catalog=Obeidat;Integrated Security=True;pooling=True"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
</configuration>
